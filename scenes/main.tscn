vec2 ps = TEXTURE_PIXEL_SIZE;
float a;
float maxa=col.a;
float mina=col.a;
a=tex(TEXTURE,UV+vec2(0,-aura_width)*ps).a;
maxa=max(a,maxa); 
mina=min(a,mina);
a=tex(TEXTURE,UV+vec2(0,aura_width)*ps).a;
maxa=max(a,maxa); 
mina=min(a,mina);
a=tex(TEXTURE,UV+vec2(-aura_width,0)*ps).a;
maxa=max(a,maxa); 
mina=min(a,mina);
a=tex(TEXTURE,UV+vec2(aura_width,0)*ps).a;
maxa=max(a,maxa); 
mina=min(a,mina);
col.rgb*=col.a;
COLOR=col;//mix(col,aura_color,maxa-mina);
float auraa=(maxa-mina);

COLOR.rgb+=aura_color.rgb*(maxa-mina);",
